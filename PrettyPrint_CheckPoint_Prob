

public class Solution {
    public int[][] prettyPrint(int A) {
        int[][] tmp=new int[2*A-1][2*A-1];
        int n=A;
        for(int i=0;i<A;i++){
            for(int j=i;j<2*A-1;j++){
                for(int k=i;k<2*A-1;k++){
                    tmp[j][k]=n;
                }
            }
        n=n-1;
        }
       return tmp;
    }
}

